{
  "name": "@python-portal/types",
  "version": "1.0.0",
  "description": "Production-grade TypeScript type definitions for Python Portal - Industry standard type safety",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "clean": "rimraf dist",
    "lint": "eslint src --ext .ts --max-warnings 0",
    "lint:fix": "eslint src --ext .ts --fix",
    "type-check": "tsc --noEmit",
    "test": "npm run lint && npm run type-check && npm run build",
    "prepublishOnly": "npm run clean && npm run build"
  },
  "keywords": [
    "typescript",
    "types",
    "python-portal",
    "microservices",
    "type-safety",
    "production"
  ],
  "author": "dstorey87 <darren@pythonportal.dev>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/dstorey87/python-portal-types.git"
  },
  "bugs": {
    "url": "https://github.com/dstorey87/python-portal-types/issues"
  },
  "homepage": "https://github.com/dstorey87/python-portal-types#readme",
  "devDependencies": {
    "@types/node": "^20.14.0",
    "@typescript-eslint/eslint-plugin": "^7.16.0",
    "@typescript-eslint/parser": "^7.16.0",
    "eslint": "^8.57.0",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "prettier": "^3.3.2",
    "rimraf": "^5.0.8",
    "typescript": "^5.5.3"
  },
  "files": [
    "dist/**/*",
    "src/**/*",
    "README.md",
    "LICENSE"
  ],
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}